<!DOCTYPE html>
<html>
	<head>
		<title>RubyWarboats</title>
		<%= stylesheet_link_tag    "application" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<ul class="hmenu">
			<%= render 'devise/menu/registration_items' %>
			<%= render 'devise/menu/login_items' %>
		</ul>
		<p class="notice">
			<%= notice %>
		</p>
		<p class="alert">
			<%= alert %>
		</p>
		<%= yield %>
		<a id="github" href="https://github.com/fcbeyer/ruby-warboats">
		  <span>Fork me on GitHub!</span>
		  <span>Fork Warboats!</span>
		</a>
	</body>
</html>
